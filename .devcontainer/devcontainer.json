{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"mounts": [
		"source=/dev/kvm,target=/dev/kvm,type=bind"
	],
	"runArgs": [
		"--network=host",
		"--cap-add=IPC_LOCK",
		"--device=/dev/kvm"
	],
	// Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/meaningful-ooo/devcontainer-features/fish:1": {}
	}
}
